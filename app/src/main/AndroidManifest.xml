<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.vaio.practica1">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Calculadora" />
        <activity android:name=".Ficheros" />
        <activity android:name=".SMS" />
        <activity android:name=".FicherosInterna" />
        <activity android:name=".FicherosRaw" />
        <activity android:name=".Intent1" />
        <activity android:name=".Intent2" />
        <activity android:name=".Bateria" />

        <receiver android:name=".MyBatteryReceiver">
            <intent-filter>
                <action android:name="android.intent.action.ACTION_POWER_CONNECTED" />
                <action android:name="android.intent.action.ACTION_POWER_DISCONNECTED" />
                <action android:name="android.intent.action.ACTION_POWER_LOW" />
                <action android:name="android.intent.action.ACTION_POWER_OKAY" />
            </intent-filter>
        </receiver>

        <activity android:name=".StartActivityForResult" />
        <activity android:name=".Omplir" />
        <activity android:name=".BaseDatos" />
        <activity android:name=".BaseDatos2"></activity>
    </application>

</manifest>